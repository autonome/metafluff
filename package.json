{
  "name": "metafluff",
  "version": "1.0.0",
  "description": "my website yo",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "render": "~/misc/ghost-render/bin/ghost-render --input ./posts/ --settings ./ghost-render.json --theme ./theme --output ./site",
    "makeprod": "mv site/index.html site/old-index.html && cp additive/*.* site/",
    "newpost": "mkdir -p posts/$(date +\"%Y/%m/%d\")/$NAME",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "node-static": "^0.7.9"
  },
